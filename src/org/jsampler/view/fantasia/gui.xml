<synth>
    <style id="titlebar">
        <state>
            <imagePainter method="panelBackground" path="res/titlebar.png" sourceInsets="3 3 3 3" stretch="true" paintCenter="true"/>
            
        </state>
    </style>
    <bind style="titlebar" type="name" key="FantasiaTitleBar" />
    
    <style id="channel">
        <state>
            <imagePainter method="panelBackground" path="res/channel.png" sourceInsets="3 3 3 3" stretch="true" paintCenter="true"/>
            
        </state>
    </style>
    <bind style="channel" type="name" key="Channel" />

    <style id="channel.screen">
        <state>
            <imagePainter method="panelBackground" path="res/channel.screen.png" sourceInsets="6 6 6 6" stretch="true" paintCenter="true"/>
            
        </state>
    </style>
    <bind style="channel.screen" type="name" key="ChannelScreen" />

    <style id="channel.options">
        <state>
            <imagePainter method="panelBackground" path="res/channel.options.png" sourceInsets="1 1 1 1" stretch="true" paintCenter="true"/>
            
        </state>
    </style>
    <bind style="channel.options" type="name" key="ChannelOptions" />

    <style id="vseparator">
        <state>
            <imagePainter method="panelBackground" path="res/line_vert.png" sourceInsets="0 0 0 0" stretch="true" paintCenter="true"/>
            
        </state>
    </style>
    <bind style="vseparator" type="name" key="VSeparator" />
    
