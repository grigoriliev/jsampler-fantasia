        <state>
            <color type="BACKGROUND" value="#000000"/>
            
            <imagePainter method="labelBorder" path="/cbLabel.png" sourceInsets="3 3 1 3" paintCenter="false"/>
            <imagePainter method="labelBackground" path="/bgLabel.png" sourceInsets="0 0 0 0" paintCenter="true"/>
            
            <insets top="3" left="3" bottom="1" right="3"/>
        </state>
    </style>
    <bind style="label" type="region" key="Label"/>
    
